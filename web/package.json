    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue-i18n": "^9.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.5.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.8",
    "vue-tsc": "^2.0.0",
    "eslint": "^10.0.2",
    "@vue/eslint-config-typescript": "^14.7.0",
    "eslint-plugin-vue": "^10.8.0",
