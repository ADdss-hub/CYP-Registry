{
  "name": "cyp-registry-web",
  "version": "1.0.8",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "preview:ci": "vite preview --host 0.0.0.0 --port 4173",
